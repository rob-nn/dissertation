isbn = {0966-6362},
issn = {09666362},
journal = {Gait and Posture},
keywords = {Bootstrap prediction,Confidence band,Gait analysis,Reliability},
number = {2},
pages = {204--212},
pmid = {15336292},
title = {{Statistical tools for clinical gait analysis}},
volume = {20},
year = {2004}
