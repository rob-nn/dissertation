issn = {00189456},
journal = {IEEE Transactions on Instrumentation and Measurement},
keywords = {Biped robotics,Force sensor,Human gait,Image processing,Sagittal balance},
number = {9},
pages = {2979--2988},
title = {{Human gait acquisition and characterization}},
volume = {58},
year = {2009}
}
